{
  "rules": {
    "indent": [
      true,
      "spaces"
    ],
    "max-line-length": [
      true,
      80
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "eofline": true,
    "jsdoc-format": true,
    "align": [
      true,
      "parameters",
      "statements"
    ],
    "whitespace": [
      true,
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-preblock",
      "check-type",
      "check-typecast"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "no-console": [
      true,
      "debug",
      "dir",
      "profile"
    ],
    "no-debugger": true,
    "no-empty": true,
    "no-eval": true,
    "no-trailing-whitespace": true,
    "no-unused-expression": true,
    "no-var-keyword": true,
    "trailing-comma": [
      true, {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-switch-case-fall-through": true,
    "switch-default": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "interface-over-type-literal": true
  }
}
